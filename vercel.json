{
  "buildCommand": "cd americanbox-react && npm ci && npm run build",
  "outputDirectory": "americanbox-react/dist",
  "installCommand": "npm install",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}