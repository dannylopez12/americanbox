{
  "buildCommand": "cd americanbox-react && npm install && npm run build",
  "outputDirectory": "americanbox-react/dist",
  "installCommand": "cd americanbox-react && npm install",
  "functions": {
    "api/*.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}